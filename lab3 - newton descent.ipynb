   "source": [
    "from derivatives import hessian\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "def f_prueba(x):\n",
    "    x1 = x[0]\n",
    "    x2 = x[1]\n",
    "    fx = (x1**2)*(x2**2) + 8*(x1)*(x2)\n",
    "    return fx"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[ 1.9999824  12.00001876]\n",
      " [12.00001876  1.9999824 ]]\n"
     ]
    }
   ],
   "source": [
    "x = np.array([1., 1.])\n",
    "print(hessian(f_prueba, x))"
   ]
  }
